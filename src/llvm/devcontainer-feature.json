{
    "name": "LLVM",
    "id": "llvm",
    "version": "1.0.0",
    "capAdd": [
        "SYS_PTRACE"
    ],
    "options": {
        "LOCAL_WORKSPACE_FOLDER=${LOCAL_WORKSPACE_FOLDER}"
    },
    "description": "Install LLVM compiler toolchain",
    "privileged": true,
    "securityOpt": [
        "apparmor=unconfined"
    ]
}
